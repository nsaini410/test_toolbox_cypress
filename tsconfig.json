{
  "compilerOptions": {
    "target": "ES6",
    "module": "node16",
    "moduleResolution": "nodenext",
    "strict": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*"]
    },
    "types": ["cypress", "node"]
  },
  "include": [
    "cypress/**/*.ts",
    "node_modules/cypress",
    "*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}